{
  "name": "News Article",
  "baseSelector": ".body-post.clear",
  "fields": [
    {
      "name": "title",
      "selector": ".home-title",
      "type": "text"
    },
    {
      "name": "date",
      "selector": ".h-datetime",
      "type": "text"
    },
    {
      "name": "tags",
      "selector": ".h-tags",
      "type": "text"
    },
    {
      "name": "description",
      "selector": ".home-desc",
      "type": "text"
    },
    {
      "name": "image_url",
      "selector": ".img-ratio img",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "article_url",
      "selector": ".story-link",
      "type": "attribute",
      "attribute": "href"
    }
  ]
}